{
    "name": "Morning_Star New Tab",
    "manifest_version": 3,
    "description": "Latest News on Morning_Star.",
    "version": "1.3",
    "background": {
      "service_worker": "background.js"
    },
    

    "permissions": [
      "activeTab",
      "storage",
      "tabs"
    ],
    "host_permissions": [
   
    ],
    "action": {
        "default_title": "Morning_Star New Tab"
    },
    "offline_enabled": true,
    "chrome_url_overrides": {
      "newtab": "sample.html"
    }
  }